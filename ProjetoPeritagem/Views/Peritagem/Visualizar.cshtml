                        <th scope="col">Peso</th>
                        <th scope="col">Material</th>
                        <th scope="col">Situação</th>
                        <th scope="col"></th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        foreach (var bj in Model.listItemDiverso)
                        {
                            if (bj.TipoItem.Id == 7 || bj.TipoItem.Nome == "Tampa / Espaçador")
                            {
                                <tr>
                                    <td>@bj.Id</td>
                                    <td>@bj.Descricao</td>
                                    <td>@bj.Disposicao.Nome</td>
