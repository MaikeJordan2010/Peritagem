@model ProjetoPeritagem.ViewHelper.ViewHelperPeritagem
@{
    ViewData["Title"] = "Visualizar";
}

<partial name="_SubMenu" />

<h2 id="titulo-form">Visualizar Peritagem</h2>
<div id="main">
    <table id="tabela_visualizar">
        <tbody>
            <tr class="tabela_visualizar_titulo">
                <td colspan="6"><h5>Informações da Peritagem</h5></td>
            </tr>
            <tr>
                <td colspan="3"><strong>Empresa</strong></td>
                <td colspan="3"><img src="../@Model.peritagem.Cliente.Icone" style="width:40px;" />   @Model.peritagem.Cliente.Nome</td>
            </tr>

            <tr>
                <td><strong>Nota Fiscal</strong></td>
                <td>@Model.peritagem.Nfe</td>
                <td><strong>Nº Contrato</strong></td>
                <td>@Model.peritagem.NumeroContrato</td>
                <td><strong>Orçamento</strong></td>
                <td>@Model.peritagem.ID_Orcamento</td>
            </tr>

            <tr>
                <td><strong>Data de Entrada:</strong></td>
                <td>@Model.peritagem.Dt_Chegada</td>
                <td><strong>Data de Recebimento:</strong></td>
                <td>@Model.peritagem.Dt_Inicio</td>
                <td><strong>Prazo de Entraga</strong></td>
                <td>@Model.peritagem.Dt_Previsao</td>
            </tr>
            <tr>
                <td><strong>Protocolo</strong></td>
                <td>@Model.peritagem.Protocolo</td>
                <td><strong>Responsável</strong></td>
                <td>@Model.peritagem.UsuarioCriador.Nome</td>
                <td><strong>Cadastro</strong></td>
                <td>@Model.peritagem.Dt_Cadastro</td>
            </tr>
            <tr>
                <td><strong>Referência</strong></td>
                <td colspan="5"> @Model.peritagem.Referencia</td>
            </tr>
            <tr>
                <td><strong>Aplicação</strong></td>
                <td colspan="5"> @Model.peritagem.Aplicacao</td>
            </tr>
            <tr>
                <td><strong>Observação</strong></td>
                <td colspan="5"> @Model.peritagem.Observacao</td>
            </tr>
            <tr>
                <td><strong>Motivo man.</strong></td>
                <td colspan="5"> @Model.peritagem.MotivoManutencao</td>
            </tr>

            <tr style="border:0; height:10px;"></tr>

        </tbody>
    </table>


    <h2 id="titulo-form">Redutor</h2>

    <table id="tabela_visualizar">

        <tbody>
            <tr class="tabela_visualizar_titulo">
                <td colspan="8"><h5> Informações do Equipamento </h5></td>
            </tr>
            @{
                if (Model.peritagem.Redutor != null)
                {
                    <tr>
                        <td><strong>ID</strong></td>
                        <td>@Model.peritagem.Redutor.Id</td>
                        <td><strong>Nome</strong></td>
                        <td>@Model.peritagem.Redutor.Nome</td>
                        <td><strong>Fabricante</strong></td>
                        <td>@Model.peritagem.Redutor.Fabricante</td>

                    </tr>
                    <tr>
                        <td><strong>Modelo</strong></td>
                        <td>@Model.peritagem.Redutor.Modelo</td>
                        <td><strong>Info</strong></td>
                        <td>@Model.peritagem.Redutor.TipoDados</td>
                        <td><strong>Redução</strong></td>
                        <td>@Model.peritagem.Redutor.Reducao</td>
                    </tr>
                    <tr>
                        <td><strong>Rotação IN</strong></td>
                        <td>@Model.peritagem.Redutor.RotacaoIN</td>
                        <td><strong>Rotação OUT</strong></td>
                        <td>@Model.peritagem.Redutor.RotacaoOUT</td>
                        <td><strong>Viscosidade</strong></td>
                        <td>@Model.peritagem.Redutor.Viscosidade</td>
                    </tr>
                    <tr>
                        <td><strong>Sentido</strong></td>
                        <td>@Model.peritagem.Redutor.Sentido</td>
                        <td><strong>Pintura PS</strong></td>
                        <td>@Model.peritagem.Redutor.PinturaPadraoSantana</td>
                        <td><strong>Volume</strong></td>
                        <td>@Model.peritagem.Redutor.Volume</td>
                    </tr>
                    <tr>
                        <td><strong>Aplicação</strong></td>
                        <td colspan="5">@Model.peritagem.Redutor.Aplicacao</td>
                    </tr>
                    <tr>
                        <td><strong>M. manut.</strong></td>
                        <td colspan="5">@Model.peritagem.Redutor.MotivoManutencao</td>
                    </tr>
                    <tr style="border:0; height:10px;"></tr>
                }
            }
        </tbody>
    </table>



    <h2 id="titulo-form">Acionamento</h2>


    <table id="tabela_visualizar">
        <tbody>

            <tr class="tabela_visualizar_titulo">
                <td colspan="8"><h5>Acionamento</h5></td>
            </tr>
            @{
                if (Model.peritagem.Acionamento != null)
                {
                    <tr>
                        <td><strong>Tipo</strong></td>
                        <td>@Model.peritagem.Acionamento.TipoAcionamento</td>
                        <td><strong>Fabricante</strong></td>
                        <td>@Model.peritagem.Acionamento.Fabricante</td>
                        <td><strong>Modelo</strong></td>
                        <td>@Model.peritagem.Acionamento.Modelo</td>
                    </tr>
                    <tr>
                        <td><strong>Rotação Polos</strong></td>
                        <td>@Model.peritagem.Acionamento.RotacaoPolos</td>
                        <td><strong>Potência</strong></td>
                        <td>@Model.peritagem.Acionamento.Potencia</td>
                        <td><strong>Carcaça</strong></td>
                        <td>@Model.peritagem.Acionamento.Carcaca</td>
                    </tr>
                    <tr>
                        <td><strong>Condição</strong></td>
                        <td>@Model.peritagem.Acionamento.Disposicao.Nome</td>
                        <td><strong>Teste do Redutor</strong></td>
                        <td>
                            @{ if (Model.peritagem.Acionamento.Teste)
                                { <p>Sim</p> }
                                else
                                {<p>Não</p> }
                            }
                        </td>
                        <td><strong>Tipo de Teste</strong></td>
                        <td></td>
                    </tr>

                }
            }

        </tbody>
    </table>

    <h2 id="titulo-form">Carcaça</h2>


    <table id="tabela_visualizar">
        <tbody>
            @{
                <tr class="tabela_visualizar_titulo">
                    <td colspan="8"><h5>Carcaça</h5></td>
                </tr>
                if (Model.peritagem.ListCarcaca != null)
                {
                    foreach (var car in Model.peritagem.ListCarcaca)
                    {
                        <tr class="tabela_visualizar_titulo_item">
                            <td colspan="8"><strong>Carcaça - @car.Id</strong></td>
                        </tr>
                        <tr>
                            <td><strong>ID</strong></td>
                            <td>@car.Id</td>
                            <td><strong>Jeteamento</strong></td>
                            <td>
                                @{
                                    if (car.Jateamento)
                                    {
                                        <p>Sim</p>
                                    }
                                    else
                                    {
                                        <p>Não</p>
                                    }
                                }
                            </td>
                            <td>Med. Alinhamento</td>
                            <td>
                                @{
                                    if (car.MedicaoAlinhamento)
                                    {
                                        <p>Sim</p>
                                    }
                                    else
                                    {
                                        <p>Não</p>
                                    }
                                }
                            </td>
                        </tr>
                        <tr>
                            <td><strong>Altura</strong></td>
                            <td>@car.Altura</td>
                            <td><strong>Largura</strong></td>
                            <td>@car.Largura</td>
                            <td><strong>Comprimento</strong></td>
                            <td>@car.Comprimento</td>
                        </tr>
                        <tr>
                            <td><strong>Condição</strong></td>
                            <td>@car.Disposicao.Nome</td>
                            <td><strong>Cadastro</strong></td>
                            <td>@car.Dt_Cadastro</td>
                            <td><strong>Produto</strong></td>
                            <td>@car.Nome</td>

                        </tr>
                        <tr>
                            <td><strong>Produto</strong></td>
                            <td colspan="5">@car.Descricao</td>
                        </tr>
                        <tr class="tabela_visualizar_titulo_item">
                            <td colspan="2">Pos.</td>
                            <td colspan="2">Medição</td>
                            <td colspan="2">Tolerância</td>
                        </tr>
                        @foreach (var med in car.Medicao)
                        {
                            <tr>
                                <td colspan="2">@med.Posicao</td>
                                <td colspan="2">@med.Medicao</td>
                                <td colspan="2">@med.Tolerancia</td>
                            </tr>
                        }
                    }

                }
            }
        </tbody>
    </table>


    <h2 id="titulo-form">Peças</h2>

    <table id="tabela_visualizar">
        <tbody>

            @{
                foreach (var it in Model.peritagem.ListPeca)
                {
                    <tr class="tabela_visualizar_titulo">
                        <td colspan="8"><h6>Peça - @it.TipoPeca.Nome </h6></td>
                    </tr>
                    <tr>
                        <td><strong>ID</strong></td>
                        <td>@it.ID_Peritagem/@it.Id</td>
                        <td><strong>Tipo</strong></td>
                        <td>@it.TipoPeca.Nome</td>
                        <td><strong>Nome</strong></td>
                        <td colspan="3">@it.Nome</td>


                    </tr>
                    <tr>
                        <td><strong>Peso</strong></td>
                        <td>@it.Peso</td>
                        <td><strong>Condição</strong></td>
                        <td>@it.Disposicao.Nome</td>
                        <td><strong>Metodo Fabricação</strong></td>
                        <td>@it.MetodoFabricacao</td>

                    </tr>
                    <tr>
                        <td><strong>Medidas</strong></td>
                        <td colspan="3">@it.Altura</td>
                        <td><strong>Trat. Term.</strong></td>
                        <td colspan="3">@it.TratamentoTermico</td>
                    </tr>
                    <tr>
                        <td><strong>Descrição</strong></td>
                        <td colspan="7">@it.Descricao</td>
                    </tr>
                    <tr></tr>
                    <tr style="border:0; height:10px;"></tr>
                }
            }

        </tbody>
    </table>


    <h2 id="titulo-form">Itens Diversos</h2>

